{
  "extends": "react-app",
	"ignorePatterns" :["dist/*"],
	"env": {
		"browser": true,
		"node": true
	},
  "rules": {
    "quotes": [1, "single"],
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/react-in-jsx-scope": 1
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  }
}

